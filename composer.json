{
    "name": "neelkanthk/laravel-easyenv",
    "description": "Easily manage and switch between multiple environment files using cli.",
    "keywords": ["laravel", "env", "environment"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Neelkanth Kaushik",
            "email": "me.neelkanth@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "Neelkanth\\Laravel\\EasyEnv\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neelkanth\\Laravel\\EasyEnv\\Providers\\EasyEnvServiceProvider"
            ]
        }
    }
}
